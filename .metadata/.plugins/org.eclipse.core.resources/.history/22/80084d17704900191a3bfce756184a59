
public class java4 {

	public static void main(String[] args) {
	
	String abc = "山田";
	System.out.println(abc + "さん");
	
	//int a1 = -5/0;
	//System.out.println(a1);
	
	int a2 = 0/3;
	System.out.println(a2);
	
	int a3 = -5*3;
	System.out.println(a3);
	
	int a4 = 4/2;
	System.out.println(a4);
	
	int a = 10/3;
	System.out.println(a);
	
	double b = -10/3;
	System.out.println(b);
	
	
	}

}
